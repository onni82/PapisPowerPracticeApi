  "components": {
    "schemas": {
      "CalorieData": {
        "required": [
          "activityLevel",
          "age",
          "gender",
          "height",
          "weight"
        ],
        "type": "object",
        "properties": {
          "gender": {
            "minLength": 1,
            "pattern": "^(male|female)$",
            "type": "string"
          },
          "weight": {
            "maximum": 300,
            "minimum": 20,
            "type": "number",
            "format": "double"
          },
          "height": {
            "maximum": 250,
            "minimum": 100,
            "type": "number",
            "format": "double"
          },
          "age": {
            "maximum": 120,
            "minimum": 5,
            "type": "integer",
            "format": "int32"
          },
          "activityLevel": {
            "minLength": 1,
            "pattern": "^(sedentary|lightly_active|moderately_active|active|very_active)$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CreateMuscleGroupDTO": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "imageUrl": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateWorkoutExerciseDTO": {
        "type": "object",
        "properties": {
          "workoutLogId": {
            "type": "integer",
            "format": "int32"
          },
          "exerciseId": {
            "type": "integer",
            "format": "int32"
          },
          "sets": {
            "type": "integer",
            "format": "int32"
          },
          "reps": {
            "type": "integer",
            "format": "int32"
          },
          "weight": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "Exercise": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "videoUrl": {
            "type": "string",
            "nullable": true
          },
          "muscleGroups": {
            "type": "array",
            "items": {
